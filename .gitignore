# Laravel specific folders and files
/vendor              # Composer dependencies
/node_modules        # Node.js dependencies
/public/build        # Compiled assets from Laravel Mix (JS, CSS, images)
public/hot           # Hot reload files from Laravel Mix
/public/storage      # Symlink to storage/app/public
/storage/*.key       # Encryption keys stored in the storage folder
/storage/pail        # Temporary files or session files in storage

# Environment configuration
.env                 # The actual environment config (contains sensitive data)
.env.backup          # Backup of .env
.env.production     # Production-specific .env file (if used)

# Testing related
.phpunit.cache       # PHPUnit cache files
.phpunit.result.cache # PHPUnit result cache files

# IDE/editor files
/.idea               # PhpStorm or JetBrains IDE settings
/.vscode             # Visual Studio Code settings
/.zed                # Zed IDE settings
/.fleet              # JetBrains Fleet IDE settings

# Logs and temporary files
*.log                # Log files
*.bak                # Backup files
*.swp                # Swap files (Vim, Emacs, etc.)
*.swo                # Swap files (Vim, Emacs, etc.)

# Homestead files (if using Laravel Homestead)
Homestead.json       # Homestead config file
Homestead.yaml       # Homestead config file

# SQLite database (use migrations instead)
database/database.sqlite  # SQLite database file (use migrations instead)

# System files generated by OS
.DS_Store            # macOS system file
Thumbs.db            # Windows system file

# JetBrains IDE specific
.idea/

# Other temporary or unnecessary files
*.swp                # Swap files (Vim, Emacs, etc.)
*.swo                # Swap files (Vim, Emacs, etc.)
node_modules
